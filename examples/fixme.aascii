Am = [a, c, e]
F  = [f, a, c]
C  = [c, e, g]
G  = [g, b, d]

AmMeasure = Am ^ Am Am ^ Am ^ ^
FMeasure  = F  ^ F  F  ^ F  F F
CMeasure  = C  C C  ^  C C  C C
GMeasure  = G  ^ G  G  G ^  G ^

Verse = AmMeasure FMeasure CMeasure GMeasure
Chorus = [Verse, a0 c0 g0]

Song = Verse Chorus

@song = Song
@duration(AmMeasure) = 1
